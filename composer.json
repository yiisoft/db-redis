{
    "name": "yiisoft/db-redis",
    "description": "Yii Framework Redis Cache, Session and ActiveRecord extension",
    "keywords": ["yii", "redis", "active-record", "cache", "session"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/db-redis/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/db-redis"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|8.0",
        "ext-mbstring": "*",
        "ext-json": "*",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/strings": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.17.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.15",
        "yiisoft/aliases": "^1.0",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/yii-event": "^3.0@dev"
    },
    "autoload": {
        "psr-4": { "Yiisoft\\Db\\Redis\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Db\\Redis\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
